</nav><!-- NavBar END -->


<!-- Bootstrap row -->
<div class="row" id="body-row">
    <!-- Sidebar -->
    <div id="sidebar-container" class="sidebar-expanded d-none d-md-block"><!-- d-* hiddens the Sidebar in smaller devices. Its itens can be kept on the Navbar 'Menu' -->
        <!-- Bootstrap List Group -->
        <ul class="list-group">
            <!-- Separator with title -->
           
            <!-- /END Separator -->
            <!-- Menu with submenu -->
            <a href="#submenu1" data-toggle="collapse" aria-expanded="false" class="bg-dark list-group-item list-group-item-action flex-column align-items-start">
                <div class="d-flex w-100 justify-content-start align-items-center">
                    
                <span class="fab fa-searchengin fa-fw mr-3"></span>

                    <span class="menu-collapsed"> Home</span>
                </div>
            </a>

                  

  
             <li class="list-group-item sidebar-separator menu-collapsed"></li>            
             <!-- /END Separator -->

          
        </ul><!-- List Group END-->
    </div><!-- sidebar-container END -->

    <!-- MAIN -->
    <div class="col">
        <div class="row" style="margin-top:1rem;">
            <div class="col-lg-4" style="font-size: 14px;">&raquo;<a href="/" style="text-decoration: none; color: rgb(3, 84, 109); ">Home</a> &raquo;Results</div>
            <div class="col-lg-4" style="background-color:rgb(6, 134, 49); border-radius:5px; padding:5px;">
                    <h5 style="text-align:center; color:white;">Recommended Results</h5>
            </div>
            <div class="col-lg-4"> <button type="button" class="btn btn-lg btn-danger" style="border-radius: 5px;"><a href="/" style="text-decoration: none; color:#fff; ">Submit Next Query</a></button> </div>
            
        </div>


        <br><br>
    <div class="container-fluid"> 
        <% result.forEach(res => {%>
        <div class="row">

            <!-- Card1 Starting   -->
            <div class="col-lg-12 col-md-12 col-sm-12 ">

                <!-- <div class="card text-black mb-3  main"> -->
                <div class="card card-shadow g">

                    <div id="iconbox" class="color1">
                    <!-- <i class="fas fa-book icon"></i> -->
                    <img style="height: 120px; width: 120px; overflow: hidden; border:3px solid #cde;" src="/public/images/<%= res.inst_image%>" style="border: 5px solid #cde" />
                    </div>

                    <div class="card-header head">

                        <div class="row">
                            <div class="col-md-2 col-sm-2 col-lg-2"></div>
                            <div class="col-md-5 col-sm-5 col-lg-5">                               
                                <div class="row">
                                    <div class="col-lg-12">
                                        <h4 style="text-align:left; font-weight:bold; "><%= res.inst_name %></h4>                                 
                                    </div>
                                </div>        
                            </div>
                            <div class="col-md-5 col-sm-5 col-lg-5">                               
                                <div class="row">
                                    <div class="col-lg-12 padd" >
                                        <h5 style="text-align:right; "><a href="<%= res.inst_url %>" target="_blank"><%= res.inst_url %></a></h5>                                 
                                    </div>
                                </div>        
                            </div>
                        </div>
                    </div>

                    <hr class="hr-prop">
                    <div class="card-body">
                        <div class="stats stats-height">
                            <div class="row">
                                <div class="col-lg-4 col-md-">       
                                    <div class="card" >
                                        <div class="card-body" style="background-color: #eee;">
                                          <h5 class="card-title">Location</h5>
                                          <p class="card-text"><%= res.inst_state %>, <%= res.inst_fulladdress %></p>
                                        </div>
                                      </div>
                                </div>
                                <div class="col-lg-4 col-md-"> 
                                    <div class="card" >                                       
                                        <div class="card-body"  style="background-color: #eee;">
                                            <h5 class="card-title">Approx. Annual Fee (AUD)</h5>
                                            <p class="card-text"><%= res.inst_fee%></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-4 col-md-">       
                                        <div class="card" >                                       
                                            <div class="card-body"  style="background-color: #eee;">
                                                <h5 class="card-title">Minimum Ielts Requirement</h5>
                                                <p class="card-text"><%= res.inst_minielts %></p>
                                            </div>
                                        </div>
                                </div>
                            </div>
                            
                            <div class="row">
                                <div class="col-md-12 wed">
                                    <button type="button"  onclick="location.href='/detail/<%=res._id%>'" class="btn btn-outline-success">More Details &raquo;</button>
                                </div>

                            </div>
                        </div>
                    </div>        
                </div>
                <div class="break"></div>

            </div>
            <!-- Card1 Finished -->   
            </div>
        <% }) %>
        </div><!-- Main Col END -->
        
    </div>
        
           
</div>

</div>

  
  

  
  <script src="/public/js/jquery.min.js"></script>
