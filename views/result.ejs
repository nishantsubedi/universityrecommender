    // to collapse sidebar at start
    SidebarCollapse();
    function displayButtons(){
        alert('kk');
        document.getElementById('mybutton').style.display="inline";
        document.getElementById('mybutton2').style.display="inline";
        // $('mybuttons').
    }
  </script>
  

</body>
